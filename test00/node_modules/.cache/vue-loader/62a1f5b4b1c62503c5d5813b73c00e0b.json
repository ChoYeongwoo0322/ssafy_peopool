r\n              style=\"margin-bottom:10px\"\r\n              >{{ props.row.date3 }} {{ props.row.time3 }}</el-radio\r\n            >\r\n          </div>\r\n          <div>\r\n            <el-button @click=\"submit(props.row, radio)\">제출</el-button>\r\n            <el-button type=\"danger\" @click=\"reject(props.row)\">거절</el-button>\r\n          </div>\r\n        </div>\r\n      </template>\r\n    </el-table-column>\r\n    <el-table-column label=\"\" prop=\"company_name\"></el-table-column>\r\n  </el-table>\r\n</template>\r\n\r\n<script>\r\nimport jwt_decode from \"jwt-decode\";\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  data() {\r\n    // 토큰가져오기\r\n    const token = localStorage.getItem(\"token\");\r\n    const decoded = jwt_decode(token);\r\n    const index = decoded.index;\r\n    // 요청받은 인터뷰\r\n    axios\r\n      .get(`https://localhost:8443/sug/${index}`, {\r\n        params: {\r\n          index: index,\r\n        },\r\n      })\r\n      .then((res) => {\r\n        console.log(res.data);\r\n        this.tableData = res.data;\r\n      })\r\n      .catch((err) => {\r\n        console.log(err);\r\n      });\r\n    return {\r\n      radio: \"1\",\r\n      submitteddate: \"\",\r\n      submittedtime: \"\",\r\n      responseform: [{ submitteddate: \"\" }, { submittedtime: \"\" }],\r\n      tableData: [\r\n        // {\r\n        //   company_id: 1,\r\n        //   company_name: \"삼성피자\",\r\n        //   company_field: \"??????????\",\r\n        //   date1: \"2021-08-30\",\r\n        //   time1: \"13:00-14:00\",\r\n        //   date2: \"2021-08-31\",\r\n        //   time2: \"13:00-14:00\",\r\n        //   date3: \"2021-08-32\",\r\n        //   time3: \"13:00-14:00\",\r\n        // },\r\n        // {\r\n        //   company_id: 2,\r\n        //   company_name: \"삼성햄버거\",\r\n        //   company_field: \"??????????\",\r\n        //   date1: \"2021-04-30\",\r\n        //   time1: \"13:00-14:00\",\r\n        //   date2: \"2021-03-30\",\r\n        //   